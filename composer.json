{
  "name": "rcm/google-analytics",
  "description": "Rcm Google Analytics",
  "type": "library",
  "license": "BSD-3-Clause",
  "keywords": [
    "rcm",
    "reliv",
    "module",
    "zf2",
    "google",
    "analytics"
  ],
  "minimum-stability": "stable",
  "homepage": "https://github.com/reliv/rcm-google-analytics",
  "authors": [
    {
      "name": "James Jervis",
      "email": "jjervis@relivinc.com",
      "homepage": "https://github.com/reliv"
    }
  ],
  "require": {
    "php": ">=7",
    "doctrine/doctrine-orm-module": "0.* || 1.*",
    "psr/container": "1.*",
    "psr/http-message": "^1.0",
    "rcm/api-lib": "~0.10",
    "rwoverdijk/assetmanager": "1.*",
    "zendframework/zend-config-aggregator": "1.*",
    "zendframework/zend-diactoros": ">=1",
    "zendframework/zend-servicemanager": "2.* || 3.*",
    "zendframework/zend-inputfilter": "2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.*",
    "squizlabs/php_codesniffer": "3.*"
  },
  "autoload": {
    "psr-4": {
      "Reliv\\RcmGoogleAnalytics\\": "src/"
    }
  },
  "suggest": {
    "rcm/core": "^1.20.0",
    "rcm/core-js": "^1.0.0",
    "rcm/i18n": "1.*",
    "rcm/user": "1.*",
    "zendframework/zend-expressive": "^2.0",
    "zendframework/zend-mvc": "2.*",
    "zendframework/zend-view": "2.*",
    "REQUIRED angular and angular translate": "For js to work"
  }
}
